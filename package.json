  "devDependencies": {
    "@sourcegraph/tsconfig": "^4.0.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "13.7.4",
    "@types/npm": "^2.0.30",
    "@types/uuid": "^3.4.4",
    "typescript": "^3.7.5"
