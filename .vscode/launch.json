	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "JS Sample",
			"cwd": "${workspaceFolder}/samples/javascript",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "jsconfig.json",
				 "--outputFormat", "line"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "TS Sample",
			"cwd": "${workspaceFolder}/samples/typescript",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "tsconfig.json",
				 "--outputFormat", "line",
				 "--installTypings",
				 "--out", "test.lsif"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "TS Sample Ref",
			"cwd": "${workspaceFolder}/samples/typescript-ref",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "client/tsconfig.json",
				 "--projectRoot", ".",
				 "--outputFormat", "line"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "lsif-npm",
			"program": "${workspaceFolder}/npm/lib/main.js",
			"cwd": "${workspaceFolder}/samples/typescript",
			"args": [
				 "--package", "./package.json",
				 "--file", "test.lsif"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/npm/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Repository Tests",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/build/azure-pipelines/tasks/lib/runLsif.js",
			"args": [
				 "${workspaceFolder}/build/azure-pipelines/test-repositories/vscode-chat.json"
			],
			"preLaunchTask": "npm: watch:build",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/build/azure-pipelines/tasks/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "LSP Types",
			"cwd": "${workspaceFolder}/../../LanguageServer/Node/types",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "tsconfig.json",
				 "--outputFormat", "line"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "LSP JSONRPC",
			"cwd": "${workspaceFolder}/../../LanguageServer/Node/jsonrpc",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "tsconfig.json",
				 "--outputFormat", "line",
				 "--stdout"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/out/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "LSP Protocol",
			"cwd": "${workspaceFolder}/../../LanguageServer/Node/protocol",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "tsconfig.json",
				 "--out", "${workspaceFolder}/tmp/protocol.lsif",
				 "--outputFormat", "line"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "mobx",
			"cwd": "${workspaceFolder}/../Playgrounds/lsif/mobx",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "tsconfig.json",
				 "--outputFormat", "line"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "rxjs",
			"cwd": "${workspaceFolder}/../Playgrounds/lsif/rxjs",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"args": [
				 "-p", "tsconfig.json",
				"--projectRoot", "./",
				 "--outputFormat", "line",
				"--out", "rxjs.lsif"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "VSLS-Guestbook",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"cwd": "${workspaceFolder}/../../connect-demo/vsls-guestbook",
			"args": [
				 "-p", "./jsconfig.json",
				 "--outputFormat", "json"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "VS Code Chat",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"cwd": "${workspaceFolder}/../Playgrounds/lsif/vscode-chat",
			"args": [
				 "-p", "./tsconfig.json",
				 "--outputFormat", "json"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "VS Code",
			"program": "${workspaceFolder}/tsc/lib/main.js",
			"cwd": "${workspaceFolder}/../vscode",
			"args": [
				"--max_old_space_size=4095",
				"-p", "./src/tsconfig.json",
				"--projectRoot", "./",
				"--outputFormat", "line",
				"--out", "vscode.lsif"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/tsc/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Blob Storage",
			"program": "${workspaceFolder}/sqlite/lib/main.js",
			"cwd": "${workspaceFolder}/../Playgrounds/lsif/mobx",
			"args": [
				"--format", "blob",
				"--projectVersion", "1.0",
				"--delete",
				"--in", "./mobx.1.lsif",
				"--out", "./mobx.db"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/sqlite/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Graph Storage",
			"program": "${workspaceFolder}/sqlite/lib/main.js",
			"cwd": "${workspaceFolder}/samples/typescript",
			"args": [
				"--format", "graph",
				"--in", "./test.lsif",
				"--out", "./test.db"
			],
			"preLaunchTask": "npm: watch",
			"sourceMaps": true,
			"outputCapture": "std",
			"outFiles": [
				"${workspaceFolder}/sqlite/lib/**/*.js"
			]
		}
	]
}